class Solution {
    public boolean doesValidArrayExist(int[] derived) {
        int x =0;
        for(int curr : derived){
            x^=curr;
        }
        return x==0;
    }
}
************************************************************
class Solution {
    public boolean doesValidArrayExist(int[] derived) {
        int x =0;
        for(int curr : derived){
            x+=curr;
        }
        return x%2== 0;
    }
}
