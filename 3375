class Solution {
    public int minOperations(int[] nums, int k) {
        int n = nums.length;
        HashSet<Integer> set = new HashSet<>();
        for(int i =0;i<n;i++){
            if(k > nums[i]){
               return -1;
            }
            set.add(nums[i]);
        }
        if(set.contains(k)){
            return set.size()-1;
        }
        return set.size();
    }
}
